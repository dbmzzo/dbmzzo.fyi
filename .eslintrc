{
  "extends": ["airbnb", "airbnb/hooks"],
  "env": {
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "jsx-a11y/label-has-associated-control": [ 2, {
      "assert": "either",
    }],
  }
}
